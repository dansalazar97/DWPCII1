{
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "hbs": "^4.2.0",
    "http-errors": "^2.0.0",
    "materialize-css": "^1.0.0",
    "morgan": "^1.10.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.15.1",
    "webpack-hot-middleware": "^2.25.4",
    "winston": "^3.11.0"
  },
  "name": "dwpcii1",
  "description": "On this repo we make a server with vist controller model using stack",
  "version": "1.0.0",
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/node": "^7.22.19",
    "@babel/preset-env": "^7.23.2",
    "babel-loader": "^9.1.3",
    "babel-watch": "^7.8.1",
    "core-js": "^3.33.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-webpack-plugin": "^4.0.1",
    "mini-css-extract-plugin": "^2.7.6"
  },
  "scripts": {
    "start": "node ./dist/bin/www",
    "dev": "NODE_ENV=development DEBUG=dwpcii:server babel-watch ./server/bin/www",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "devb": "DEBUG=dwpcii1:server && babel-watch ./server/bin/www",
    "cleans": "rm -rf ./dist",
    "cleanc": "rm -f ./public/bundle.js ./public/styles/app.css",
    "b:s": "npm run cleans && babel ./server --out-dir dist --copy-files",
    "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "build": "npm run b:c && npm run b:s",
    "clean:logs": "rm -rf ./server/logs/*.log"
  },
